<div class="error-display">
  <div class="error-content">
    <div class="error-number">
      500
    </div>
    <div class="error-text">
      <% if (typeof error!== 'undefined') { %>
        <%= error %>
      <% } else { %>
        NASA we have a problem. We will get it fixed.
      <% } %>
    </div>
  </div>
</div>
